{
  "name": "civet",
  "version": "0.1.4",
  "author": "webberg <webbery@foxmail.com>",
  "description": "素材管理软件",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "appId": "com.civet.app",
  "keywords": [
    "image",
    "manager",
    "viewer",
    "preview"
  ],
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder -p onTag",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:extend": "node .electron-vue/extension.js",
    "dev": "node extensions/build.js && npm run pack:dev_woker && node .electron-vue/dev-runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "pack": "npm run pack:main && npm run pack:renderer && npm run pack:worker",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.renderer.config.js",
    "test": "npm run e2e",
    "pack:worker": "cross-env NODE_ENV=production webpack --progress --config .electron-vue/webpack.worker.config.js",
    "pack:dev_woker": "cross-env NODE_ENV=development webpack --progress --config .electron-vue/webpack.worker.config.js",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": "npm run lint:fix && electron-builder install-app-deps",
    "publish": "node_modules/.bin/publish"
  },
  "build": {
    "productName": "Civet",
    "appId": "com.civet.app",
    "asar": true,
    "publish": [
      {
        "provider": "github",
        "owner": "webbery",
        "host": "github.com",
        "releaseType": "draft"
      }
    ],
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*",
      "!**/*.{gyp,h,hpp,cpp,c,cc,svg,md,ts}",
      "!**/*.{ilk,pdb,obj,tlog,vcxproj,filters,iobj,ipdb,exp,lib}",
      "!**/*.{o,Makefile,mk,yml,vcxproj,filters,iobj,ipdb,exp,lib}",
      "!**/Makefile",
      "!**/LICENSE"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150
        }
      ],
      "sign": false
    },
    "appImage": {
      "synopsis": "Civet",
      "category": "Utility"
    },
    "mac": {
      "identity": "v_yuanwenbin",
      "category": "public.app-category.graphics-design",
      "icon": "build/icons/icon512.icns",
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "win": {
      "icon": "build/icons/icon.ico",
      "target": "nsis"
    },
    "linux": {
      "icon": "src/main/asset/icon/icon.png",
      "target": "AppImage"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "build/icons/icon.ico",
      "uninstallerIcon": "build/icons/icon.ico",
      "installerHeaderIcon": "build/icons/icon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "perMachine": true,
      "unicode": true
    },
    "extraResources": [
      {
        "from": "./extensions-dist",
        "to": "./extensions",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "dependencies": {
    "civetkern": "^0.2.6",
    "download": "^8.0.0",
    "electron-updater": "^4.3.5",
    "element-ui": "^2.14.1",
    "image-hash": "^4.0.1",
    "import-html-entry": "^1.11.1",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "sharp": "^0.28.2",
    "tinycolor2": "^1.4.2",
    "uuid": "^8.3.1",
    "v-hotkey": "^0.9.0",
    "vue": "^2.6.12",
    "vue-electron": "^1.0.6",
    "vue-lazyload": "^1.3.3",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.4.1",
    "vuex": "^3.5.1",
    "ws": "^7.4.4",
    "yallist": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.6.0",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.6.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.6.0",
    "@babel/plugin-proposal-function-bind": "^7.6.0",
    "@babel/plugin-proposal-function-sent": "^7.6.0",
    "@babel/plugin-proposal-json-strings": "^7.6.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.6.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.6.0",
    "@babel/plugin-proposal-numeric-separator": "^7.6.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.6.0",
    "@babel/plugin-proposal-throw-expressions": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.6.0",
    "@babel/plugin-syntax-import-meta": "^7.6.0",
    "@babel/plugin-syntax-jsx": "^7.12.13",
    "@babel/plugin-transform-destructuring": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime-corejs2": "^7.11.2",
    "@vue/babel-plugin-transform-vue-jsx": "^1.2.1",
    "ajv": "^6.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "cfonts": "^2.1.2",
    "chai": "^4.3.0",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "dts-bundle": "^0.7.3",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "electron-builder-squirrel-windows": "^22.3.5",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^1.0.0",
    "electron-prebuilt": "^1.4.13",
    "electron-rebuild": "^2.0.3",
    "electron-simple-publisher": "^0.7.1",
    "element-theme": "^2.0.1",
    "element-theme-dark": "^1.0.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^8.2.1",
    "multispinner": "^0.2.1",
    "node-loader": "^1.0.0",
    "node-sass": "^4.9.2",
    "p-finally": "^2.0.1",
    "puppeteer-core": "^8.0.0",
    "require-dir": "^1.0.0",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^2.0.1",
    "strip-eof": "^2.0.0",
    "style-loader": "^0.21.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.17",
    "tweetnacl": "^1.0.3",
    "typescript": "^4.1.5",
    "url-loader": "^2.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.15.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3",
    "worker-loader": "^3.0.0"
  }
}
